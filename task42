/* 
Вывести на экран таблицу умножения 10х10 используя цикл while.
Числа разделить пробелом.
*/

public class Solution {
    public static void main(String[] args) {
        int i = 1;
        int j = 1;
        
        while (i <= 10) {
        
            int a = 1;
            while (a <= 10) {
                System.out.print(i * a + " ");
                a++;
            }
        System.out.println(" ");
            i++;
        }
    }
}
